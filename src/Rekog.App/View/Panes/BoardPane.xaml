    <TabControl BorderThickness="0" TabStripPlacement="Left">
        <TabControl.Resources>
            <Style x:Key="TabItemTextStyle"
                   BasedOn="{StaticResource {x:Type TabItem}}"
                   TargetType="{x:Type TabItem}">
                <Setter Property="IsEnabled" Value="False" />
                <Setter Property="HeaderTemplate">
                    <Setter.Value>
                        <DataTemplate>
                            <TextBlock Foreground="{StaticResource SystemControlForegroundBaseHighBrush}"
                                       Style="{StaticResource BaseTextBlockStyle}"
                                       Text="{Binding}" />
                        </DataTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
            <Style TargetType="{x:Type HeaderedContentControl}">
                <Setter Property="IsTabStop" Value="False" />
                <Setter Property="HeaderTemplate">
                    <Setter.Value>
                        <DataTemplate>
                            <TextBlock Margin="0,16,0,12"
                                       Style="{StaticResource BaseTextBlockStyle}"
                                       Text="{Binding}" />
                        </DataTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
            <Style TargetType="{x:Type ui:SimpleStackPanel}">
                <Setter Property="Spacing" Value="12" />
            </Style>
            <Style TargetType="{x:Type ScrollViewer}">
                <Setter Property="VerticalScrollBarVisibility" Value="Auto" />
            </Style>
            <Style BasedOn="{StaticResource {x:Type ui:NumberBox}}" TargetType="{x:Type ui:NumberBox}">
                <Setter Property="AcceptsExpression" Value="True" />
                <Setter Property="TextAlignment" Value="Left" />
            </Style>
            <Style x:Key="GroupMenuItemStyle"
                   BasedOn="{StaticResource {x:Type MenuItem}}"
                   TargetType="{x:Type MenuItem}">
                <Setter Property="IsEnabled" Value="False" />
            </Style>
        </TabControl.Resources>
