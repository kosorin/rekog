        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:ui="http://schemas.modernwpf.com/2019"
        xmlns:v="clr-namespace:Rekog.App.View"
        Title="Rekog"
        DataContext="{Binding Source={StaticResource MainViewModel}}"
        Width="1600"
        Height="1000"
        WindowStartupLocation="CenterScreen"
        WindowState="Maximized"
        mc:Ignorable="d">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition Width="12" />
            <ColumnDefinition Width="450" />
        </Grid.ColumnDefinitions>
        <v:BoardView x:Name="Board"
                     DataContext="{Binding Board}"
                     Grid.Column="0"
                     Grid.ColumnSpan="2" />
        <ui:ThemeShadowChrome Grid.Column="0"
                              Margin="8,16"
                              HorizontalAlignment="Right"
                              VerticalAlignment="Top"
                              Visibility="{Binding Board, Converter={StaticResource NotNullToVisibilityConverter}}">
            <Border Padding="12" Background="{StaticResource SystemControlBackgroundAltHighBrush}">
                <ui:SimpleStackPanel Orientation="Horizontal" Spacing="12">
                    <ui:SplitButton Command="{Binding Board.AddKeyCommand}" Content="Add key" />
                    <Button Command="{Binding Board.DeleteSelectedKeysCommand}" Content="Delete keys" />
                </ui:SimpleStackPanel>
            </Border>
        </ui:ThemeShadowChrome>
        <ui:ThemeShadowChrome Grid.Column="2">
            <ui:ThemeShadowChrome.Style>
                <Style TargetType="{x:Type ui:ThemeShadowChrome}">
                    <Setter Property="Visibility" Value="Visible" />
                    <Style.Triggers>
                        <Trigger Property="ActualWidth" Value="0">
                            <Setter Property="Visibility" Value="Hidden" />
                        </Trigger>
                    </Style.Triggers>
                </Style>
            </ui:ThemeShadowChrome.Style>
            <TabControl Background="{StaticResource SystemControlForegroundChromeHighBrush}"
                        BorderThickness="0"
                        TabStripPlacement="Left">
                <TabControl.Resources>
                    <Style x:Key="TabItemTextStyle"
                           BasedOn="{StaticResource {x:Type TabItem}}"
                           TargetType="{x:Type TabItem}">
                        <Setter Property="IsEnabled" Value="False" />
                        <Setter Property="HeaderTemplate">
                            <Setter.Value>
                                <DataTemplate>
                                    <TextBlock Foreground="Black"
                                               Style="{StaticResource BaseTextBlockStyle}"
                                               Text="{Binding}" />
                                </DataTemplate>
                            </Setter.Value>
                        </Setter>
                    </Style>
                    <Style TargetType="{x:Type HeaderedContentControl}">
                        <Setter Property="IsTabStop" Value="False" />
                        <Setter Property="HeaderTemplate">
                            <Setter.Value>
                                <DataTemplate>
                                    <TextBlock Margin="0,12"
                                               Style="{StaticResource BaseTextBlockStyle}"
                                               Text="{Binding}" />
                                </DataTemplate>
                            </Setter.Value>
                        </Setter>
                    </Style>
                    <Style TargetType="{x:Type ui:SimpleStackPanel}">
                        <Setter Property="Spacing" Value="12" />
                    </Style>
                    <Style TargetType="{x:Type ScrollViewer}">
                        <Setter Property="VerticalScrollBarVisibility" Value="Auto" />
                        <Setter Property="Background" Value="{StaticResource SystemControlBackgroundAltHighBrush}" />
                    </Style>
                </TabControl.Resources>
                <TabItem Header="Board">
                    <ui:TabItemHelper.Icon>
                        <ui:FontIcon Glyph="&#xE144;" />
                    </ui:TabItemHelper.Icon>
                    <ScrollViewer>
                        <ui:SimpleStackPanel Margin="16,8" Spacing="16">
                            <HeaderedContentControl DataContext="{Binding Board.Form}"
                                                    Header="Board"
                                                    IsEnabled="{Binding IsSet}">
                                <ui:SimpleStackPanel>
                                    <TextBox ui:ControlHelper.Header="Name" Text="{Binding Name.Value, Mode=TwoWay}" />
                                    <TextBox ui:ControlHelper.Header="Author" Text="{Binding Author.Value, Mode=TwoWay}" />
